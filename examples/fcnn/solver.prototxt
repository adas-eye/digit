#net: "train_val.prototxt"
net: "fcn11.prototxt"
test_iter: 200
# make test net, but don't invoke it from the solver itself
test_interval: 10000
display: 20
average_loss: 20

base_lr: 1e-7
lr_policy: "step"
gamma: 0.1
stepsize: 5000
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 5
max_iter: 80000
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "models/fcn11/fcn11"
test_initialization: false